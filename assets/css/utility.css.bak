*{
	padding:0;
	margin:0;
	box-sizing: border-box;
	font-family: 'Poppins', sans-serif;
}

.container{
	 width:1100px;
	 margin:0 auto;
}

ul{
	list-style:none;
}

a:link, 
a:visited{
	text-decoration:none;
}

.row,
.row::after,
.row::before {
	width:100%;
	display:table;
	clear:both;
}
/*
